@using CS_499_Project.Object_Classes

<!DOCTYPE html>
<style type="text/css">
    td
    {
        padding:0 30px 0 30px;
    }
</style>
<html>
<head>
    <title>title</title>
</head>
<body>
<center>
    <font size="10">
    <table>
        <tr>
            <th> Acct To: </th>
            <th> To Name: </th>
            <th> Acct From: </th>
            <th> From Name: </th>
            <th> Amount: </th>
            <th> Note: </th>
            <th> Amount: </th>
        </tr>
        @foreach (TransactionInterface obj in ViewBag.Transactions)
        {
            <tr>
                <td> @obj.acct_to </td>
                <td> @(new Database().GetAcctName(obj.acct_to)) </td>
                <td> @obj.acct_from </td>
                <td> @(new Database().GetAcctName(obj.acct_from)) </td>
                <td> @obj.amount </td>
                <td> @obj.note </td>
                <td> @obj.transaction_time.ToString("d") </td>
            </tr>
        }

    </table>
    </font>
</center>
<div>
</div>
</body>
</html>