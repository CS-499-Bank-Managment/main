
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>

</head>

<body>
    <h1>My Accounts</h1>

    @foreach (KeyValuePair<long, string> entry in ViewBag.accounts)
    {
        <p>
            <span>@entry.Key</span>
            <span>@entry.Value</span>
        </p>
    }

    <a href="../DeleteAccount/@ViewBag.username">Delete An Account</a>

</body>
    
</html>