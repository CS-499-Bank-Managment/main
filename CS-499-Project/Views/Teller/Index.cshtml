<!DOCTYPE html>
<html>
	<head>
		<style>
			</style>
		Welcome to the Teller Index page, @ViewBag.User
	</head>
<body>
<h1>Customer Search</h1>
<br><br><br>
Find a customer:&nbsp;&nbsp;<input type="text"/>
<br><br>
<table>
	<tr>
		<th>Customer Number</th>
		<th>Customer Name</th>
		<th>Customer Email</th>
		<th>Select Customer</th>
	</tr>
	<tr>
		<td>123456</td>
		<td>Natalie Euley</td>
		<td>natalie.euley@gmail.com</td>
		<td><button>SELECT</button></td>
	</tr>
</table>



<br/><br/>
<form method="post">
	Find a customer by username: <input name="username" type="text"/>
	<input type="submit"/>	
</form>

<table>
	@foreach (KeyValuePair<string,string> pair in ViewBag?.results)
	{
	String.Format("<tr><td>{0}</td><td>{1}</td></tr>", pair.Key, pair.Value.ToString());
	}
 </table>
</body>
</html>
